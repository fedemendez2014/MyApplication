<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.tecnoinfsanjose.tareaandroiduno.Views.Fragments.FragmentRegistrar"
    android:background="@drawable/ic_launcher_background"
    android:orientation="vertical"
    android:id="@+id/registrar"
    >


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView12"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_alignBottom="@+id/ciudad"
            android:layout_alignStart="@+id/textView8"
            android:text="Ciudad:"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="34dp"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/textView10"
            android:layout_marginBottom="157dp"
            android:text="Dirección:"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/spinner2"
            android:layout_alignStart="@+id/textView12"
            android:text="Rol:"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/textView11"
            android:layout_marginBottom="210dp"
            android:text="Teléfono:"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:layout_alignStart="@+id/textView6"
            android:layout_marginBottom="257dp"
            android:text="Documento:"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/button3"
            android:layout_marginTop="278dp"
            android:text="Repetir Contraseña:"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/textView6"
            android:layout_marginTop="235dp"
            android:text="Contraseña:"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/textView11"
            android:layout_marginTop="190dp"
            android:text="Apellido:"
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/textView11"
            android:layout_marginTop="144dp"
            android:text="Nombre: "
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="62dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/textView7"
            android:background="@android:color/holo_blue_dark"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme" />

        <Button
            android:id="@+id/button3"
            android:layout_width="40dp"
            android:layout_height="36dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="14dp"
            android:layout_marginTop="11dp"
            android:background="@drawable/previous" />

        <Button
            android:id="@+id/button4"
            android:layout_width="40dp"
            android:layout_height="36dp"
            android:layout_alignParentEnd="true"
            android:layout_alignTop="@+id/button3"
            android:layout_marginEnd="26dp"
            android:background="@drawable/tick" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/textView12"
            android:layout_marginTop="100dp"
            android:text="E-mail: "
            android:textColor="@android:color/white"
            android:textSize="18sp" />

        <EditText
            android:id="@+id/ciudad"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/button4"
            android:layout_below="@+id/textView8"
            android:layout_marginTop="-156dp"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/direccion"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView8"
            android:layout_alignEnd="@+id/button4"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/documento"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView11"
            android:layout_alignEnd="@+id/button4"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/apellido"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView3"
            android:layout_alignEnd="@+id/button4"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/nombre"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView4"
            android:layout_alignStart="@+id/correo"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/correo"
            android:layout_width="163dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView"
            android:layout_alignEnd="@+id/button4"
            android:ems="10"
            android:inputType="textPersonName" />

        <EditText
            android:id="@+id/password2"
            android:layout_width="157dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView6"
            android:layout_alignEnd="@+id/button4"
            android:ems="10"
            android:inputType="textPassword" />

        <EditText
            android:id="@+id/password"
            android:layout_width="157dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView5"
            android:layout_alignEnd="@+id/button4"
            android:ems="10"
            android:inputType="textPassword" />

        <EditText
            android:id="@+id/telefono"
            android:layout_width="164dp"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView10"
            android:layout_alignEnd="@+id/button4"
            android:ems="10"
            android:inputType="phone" />

        <Spinner
            android:id="@+id/spinner2"
            android:layout_width="165dp"
            android:layout_height="39dp"
            android:layout_alignEnd="@+id/button4"
            android:layout_below="@+id/ciudad" />

    </RelativeLayout>

</FrameLayout>
